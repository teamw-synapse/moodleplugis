<script>
  // Function to show/hide charts based on selected option
  document.getElementById('chart-select').addEventListener('change', function() {
    var selectedChart = this.value;
    if (selectedChart === 'pie') {
      document.getElementById('pie-chart-container').style.display = 'block';
      document.getElementById('bar-chart-container').style.display = 'none';
    } else if (selectedChart === 'bar') {
      document.getElementById('pie-chart-container').style.display = 'none';
      document.getElementById('bar-chart-container').style.display = 'block';
    }
  });

  // Pie Chart
  var pieChartCtx = document.getElementById('pie-chart').getContext('2d');
  var pieChart = new Chart(pieChartCtx, {
    type: 'pie',
    data: {
      labels: ['Category 1', 'Category 2', 'Category 3', 'Category 4'],
      datasets: [{
        data: [20, 30, 15, 35],
        backgroundColor: ['#FF6384', '#36A2EB', '#FFCE56', '#4BC0C0']
      }]
    }
  });

  // Bar Chart
  var barChartCtx = document.getElementById('bar-chart').getContext('2d');
  var barChart = new Chart(barChartCtx, {
    type: 'bar',
    data: {
      labels: ['Category 1', 'Category 2', 'Category 3', 'Category 4'],
      datasets: [{
