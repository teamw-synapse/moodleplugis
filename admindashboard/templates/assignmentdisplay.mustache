
{{#response}}
    {{#viewtypeCard}}
<table  id="data" class="generaltable">
  <thead>
    <tr>

      <th>Assignment Name</th>
      <th>Course Name</th>
      <th>User Count</th>
      <th>Submitted Count</th>
      
    </tr>
  </thead>
  <tbody>
   {{#records}}
    <tr>

      <td><a style= "color: darkturquoise; cursor: pointer;text-decoration: underline; " href="{{assign_url}}" target="_blank">{{assign_name}}</a></td>
      <td>{{course_name}}</td> 
      <td><a style= "color: darkturquoise;cursor: pointer; text-decoration: underline;" onclick="(function(e){ require('block_admindashboard/assignuser').init({assignid:{{assignid}},courseid:{{courseid}},userid:{{userid}} }) })(event)">{{user_count}}</a></td>
      <td><a style= "color: darkturquoise;cursor: pointer; text-decoration: underline;" onclick="(function(e){ require('block_admindashboard/assignsub').init({assignid:{{assignid}},courseid:{{courseid}},userid:{{userid}} }) })(event)">{{submission_count}}</a></td>

    </tr>
   {{/records}}
  </tbody>
</table>
 {{/viewtypeCard}}
{{/response}}