    {{#viewtypeCard}}
<table  id="data" class="generaltable">
  <thead>
    <tr>

      <th>Forum Name</th>
      <th>Course Name</th>
      <th>User Count</th>
      <th>Discussion Count</th>
      
    </tr>
  </thead>
  <tbody>
   {{#records}}
    <tr>
      <td><a style= "color: darkturquoise; cursor: pointer; text-decoration: underline;" href="{{forum_url}}" target="_blank">{{forum_name}}</a></td>
      <td>{{course_name}}</td>
      <td><a style= "color: darkturquoise;cursor: pointer;text-decoration: underline;" onclick="(function(e){ require('block_admindashboard/forumuser').init({forumid:{{forumid}},courseid:{{courseid}},userid:{{userid}} }) })(event)">{{user_count}}</a></td>
      <td><a style= "color: darkturquoise; cursor: pointer; text-decoration: underline;" onclick="(function(e){ require('block_admindashboard/forumdisc').init({forumid:{{forumid}},courseid:{{courseid}},userid:{{userid}} }) })(event)">{{discussion_count}}</a></td>
    </tr>
   {{/records}}
  </tbody>
</table>
{{/viewtypeCard}}
{{/response}}