{{#response}}
    {{#viewtypeCard}}
<table  id="data" class="generaltable">
  <thead>
    <tr>
      <th>Course Name</th>
      <th>Forum Name</th>
      <th>Discussion Name</th>
      <th>User Count</th>
      <th>Submission Count</th>
      
    </tr>
  </thead>
  <tbody>
   {{#records}}
    <tr>
      <td>{{course_name}}</td>
      <td>{{forum}}</td>
      <td><a style= "color: darkturquoise; cursor: pointer; text-decoration: underline;" href="{{discussion_url}}" target="_blank">{{forum_discussion}}</a></td>
      <td><a style= "color: darkturquoise; cursor: pointer;text-decoration: underline;" onclick="(function(e){ require('block_admindashboard/forumuser').init({forumid:{{forumid}},courseid:{{courseid}},userid:{{userid}} }) })(event)">{{user_count}}</a></td>
      <td><a style= "color: darkturquoise; cursor: pointer;  text-decoration: underline;" onclick="(function(e){ require('block_admindashboard/forumsub').init({forumid:{{forumid}},discussionid:{{discussion_id}},courseid:{{courseid}},userid:{{userid}} }) })(event)">{{submission_count}}</a></td>
    </tr>
   {{/records}}
  </tbody>
</table>
{{/viewtypeCard}}
{{/response}}